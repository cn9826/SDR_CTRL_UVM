#!/bin/csh -f

if(! -e work) then
   vlib work
else
   \rm -rf work
   vlib work
endif
if($1 == "core") then # run SDRAM Core level test case
   vlog -work work +define+$2 -f filelist_core.f
   vsim tb_core 
   run -all
else # Run SDRAM Top Level test cases
   vlog -work work +define+$2 -f filelist_top.f
endif

